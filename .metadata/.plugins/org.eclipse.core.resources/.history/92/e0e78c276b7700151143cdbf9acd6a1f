
public class Assignment_2 {

	public static void main(String[] args) {
		int a = 1, b = 0;

		while (a <= 100){
			
			int c = 1;
			while (c <= 100){
				if (a % c == 0){
					b++;
					c++;
				}
				else 
					c++;
				}
			if (b == 2)
				System.out.println(a);
				b = 0;
			a++;
			
			System.out.println("Prime number between 1 and 100");
			
		}
	}

}
