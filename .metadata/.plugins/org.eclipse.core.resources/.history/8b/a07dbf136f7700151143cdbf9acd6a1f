		   public static void main(String[] args) {       
			      int no = Integer.parseInt(args[0]);  
			  
			     // count from i = 1 to 10  
			      int i = 1;  
			      while (i <= 10) {  
			         System.out.println(i*no);   
			          i++;  
			      }  
			   }  
			}
